@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <title>Vipnet Asansör</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="" name="keywords">
    <meta content="" name="description">

    <!-- Favicon -->
    <link href="~/Vipnet Asansör/img/Vip_Asansor_Logo2.png" rel="icon">

    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@400;600&family=Ubuntu:wght@500;700&display=swap"
          rel="stylesheet">

    <!-- Icon Font Stylesheet -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet">

    <!-- Libraries Stylesheet -->
    <link href="~/Vipnet Asansör/lib/animate/animate.min.css" rel="stylesheet">
    <link href="~/Vipnet Asansör/lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">

    <!-- Customized Bootstrap Stylesheet -->
    <link href="~/Vipnet Asansör/css/bootstrap.min.css" rel="stylesheet">

    <!-- Template Stylesheet -->
    <link href="~/Vipnet Asansör/css/style.css" rel="stylesheet">
</head>

<body>
    <!-- Spinner Start -->
    <div id="spinner"
         class="show bg-white position-fixed translate-middle w-100 vh-100 top-50 start-50 d-flex align-items-center justify-content-center">
        <div class="spinner-grow text-primary" style="width: 3rem; height: 3rem;" role="status">
            <span class="sr-only">Loading...</span>
        </div>
    </div>
    <!-- Spinner End -->
    <div class="container">
        @await Component.InvokeAsync("TopbarList")
    </div>

    <!-- Navbar Start -->
    <div class="container-fluid sticky-top ">
        <div class="container">

            <nav class="navbar navbar-expand-lg navbar-dark p-0 ">
                <a href="index" class="navbar-brand">
                    <img src="~/Vipnet Asansör/img/Vip_Asansor_Beyaz.png" alt="">
                </a>
                <button type="button" class="navbar-toggler ms-auto me-0" data-bs-toggle="collapse"
                        data-bs-target="#navbarCollapse">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarCollapse">
                    <div class="navbar-nav ms-auto">
                        <a href="index" class="nav-item nav-link active">Anasayfa</a>
                        <a href="about" class="nav-item nav-link">Hakkımızda</a>
                        <div class="nav-item dropdown">
                            <a href="#" class="nav-link dropdown-toggle" data-bs-toggle="dropdown">Ürünler</a>
                            <div class="dropdown-menu bg-light mt-2">
                                <a href="vpc-edc-01-asansor-kontrol-karti.html" class="dropdown-item">VPC-EDC-01 Asansör Kontrol Kartı</a>
                                <a href="kapi-fotoseli.html" class="dropdown-item">Kapı Fotoseli</a>
                                <a href="mono-stabil.html" class="dropdown-item">Mono Stabil</a>

                            </div>
                        </div>
                        <a href="arge" class="nav-item nav-link">Arge</a>

                        <a href="contact" class="nav-item nav-link">İletişim</a>
                    </div>
                    <butaton type="button" class="btn text-white p-0 d-none d-lg-block" data-bs-toggle="modal"
                             data-bs-target="#searchModal"><i class="fa fa-search"></i></butaton>
                </div>
            </nav>
        </div>
    </div>
    <!-- Navbar End -->
    <!-- Feature Start -->
      
       @await Component.InvokeAsync("FeatureList")
    <!-- Feature End -->
    <!-- Full Screen Search Start -->
    <div class="modal fade" id="searchModal" tabindex="-1">
        <div class="modal-dialog modal-fullscreen">
            <div class="modal-content" style="background: rgba(20, 24, 62, 0.7);">
                <div class="modal-header border-0">
                    <button type="button" class="btn btn-square bg-white btn-close" data-bs-dismiss="modal"
                            aria-label="Close"></button>
                </div>
                <div class="modal-body d-flex align-items-center justify-content-center">
                    <div class="input-group" style="max-width: 600px;">
                        <input type="text" class="form-control bg-transparent border-light p-3"
                               placeholder="Type search keyword">
                        <button class="btn btn-light px-4"><i class="bi bi-search"></i></button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Full Screen Search End -->
    <!-- About Start -->
     @await Component.InvokeAsync("HeaderList")
   
    <!-- About End -->
    <!-- Ürünler Start -->
    @await Component.InvokeAsync("ProductList")

    
    <!-- Ürünler End -->
    <!-- Portfolio Start -->
    @await Component.InvokeAsync("PortfolioList")

   
    <!-- Portfolio End -->
    <!-- Questions Start -->
    @await Component.InvokeAsync("QuestionsList")

   
    <!-- Questions End -->





    <!-- Brands Start -->
    <div class="container-fluid bg-light py-5 markalarimiz">
        <div class="container ">
            <div class="row ">
                <h2 class="mb-4 text-center">Markalarımız</h2>
                <div class="border-bottom"></div>
                <div class="row align-items-center">

                    <div class="col-lg-6 wow fadeIn" data-wow-delay="0.1s">
                       
                        @await Component.InvokeAsync("MasterBrandList")<!-- Endüstriyel -->

                    </div>
                    <div class="col-lg-6 wow fadeIn" data-wow-delay="0.5s">
                        <div class="owl-carousel testimonial-carousel border-start border-primary">
                     
                            @await Component.InvokeAsync("BrandList")<!-- Markalarımız  -->
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Footer Start -->
    <div class="container-fluid bg-primary text-white-50 footer">
        <div class="container py-5">
            <div class="row g-5">
                <div class="col-md-6 col-lg-3 wow fadeIn" data-wow-delay="0.1s">
                    <a href="index" class="d-inline-block mb-3">
                        <img class="w-75" src="~/Vipnet Asansör/img/Vip_Asansor_Beyaz.png" alt="">
                    </a>
                    <p class="mb-0">Endüstriyel Elektrik Elektronik San. ve Tic. A.Ş</p>
                </div>
                <div class="col-md-6 col-lg-3 wow fadeIn" data-wow-delay="0.3s">
                    <h5 class="text-white mb-4">Menü</h5>
                    <div class="ms-0 border-bottom"></div>
                    <a class="btn btn-link" href="index">Anasayfa</a>
                    <a class="btn btn-link" href="about">Hakkımızda</a>
                    <div class="nav-item dropdown">
                        <a href="#" class="btn btn-link dropdown-toggle" data-bs-toggle="dropdown">Ürünler</a>
                        <div class="dropdown-menu bg-light mt-2">
                            <a href="vpc-edc-01-asansor-kontrol-karti.html" class="dropdown-item">VPC-EDC-01 Asansör Kontrol Kartı</a>
                            <a href="kapi-fotoseli.html" class="dropdown-item">Kapı Fotoseli</a>
                            <a href="mono-stabil.html" class="dropdown-item">Mono Stabil</a>
                        </div>
                    </div>
                    <a class="btn btn-link" href="arge">Arge</a>
                    <a class="btn btn-link" href="contact">İletişim</a>


                </div>
                <div class="col-md-6 col-lg-3 wow fadeIn" data-wow-delay="0.5s">
                    <h5 class="text-white mb-4">İletişim</h5>
                    <div class="ms-0 border-bottom"></div>
                    <p><i class="fa fa-map-marker-alt me-3"></i>3. Organize Sanayi Bölgesi Evrenköy Caddesi 3. Sokak No:5 Selçuklu / Konya - TÜRKİYE</p>
                    <p><i class="fa fa-phone-alt me-3"></i>+90 332 239 09 00</p>
                    <p><i class="fa fa-envelope me-3"></i>info@vipnetasansor.com</p>
                    <div class="d-flex pt-2">

                        <a class="btn btn-outline-light btn-social" href=""><i class="fab fa-facebook-f"></i></a>
                        <a class="btn btn-outline-light btn-social" href=""><i class="fab fa-youtube"></i></a>
                        <a class="btn btn-outline-light btn-social" href=""><i class="fab fa-instagram"></i></a>
                        <a class="btn btn-outline-light btn-social" href=""><i class="fab fa-linkedin-in"></i></a>
                    </div>
                </div>
                <div class="col-md-6 col-lg-3 wow fadeIn" data-wow-delay="0.7s">
                    <h5 class="text-white mb-4">Google Haritalar</h5>
                    <div class="ms-0 border-bottom"></div>
                    <iframe src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d393.1858623299959!2d32.623358!3d37.965765!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x14d09388a162e307%3A0x7f45e6d34bcaad49!2sEnd%C3%BCstriyel%20Elektrik!5e0!3m2!1str!2str!4v1717738508204!5m2!1str!2str"
                            width="300" height="200" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
                </div>
            </div>
        </div>

        <div class="container wow fadeIn" data-wow-delay="0.1s">
            <div class="copyright">
                <div class="row">
                    <div class="col-md-6 text-center text-md-start mb-3 mb-md-0">
                        &copy; 2024 Vipnet Asansör Tüm Hakları Saklıdır.
                    </div>
                    <div class="col-md-6 text-center text-md-end">
                        <!-- Buraya diğer içeriklerinizi ekleyebilirsiniz -->
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Footer End -->
    <!-- Back to Top -->
    <a href="#" class="btn btn-lg btn-outline-primary btn-lg-square back-to-top pt-2"><i class="bi bi-arrow-up"></i></a>


    <!-- JavaScript Libraries -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.1/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/Vipnet Asansör/lib/wow/wow.min.js"></script>
    <script src="~/Vipnet Asansör/lib/easing/easing.min.js"></script>
    <script src="~/Vipnet Asansör/lib/waypoints/waypoints.min.js"></script>
    <script src="~/Vipnet Asansör/lib/counterup/counterup.min.js"></script>
    <script src="~/Vipnet Asansör/lib/owlcarousel/owl.carousel.min.js"></script>

    <!-- Template Javascript -->
    <script src="~/Vipnet Asansör/js/main.js"></script>
</body>

</html>